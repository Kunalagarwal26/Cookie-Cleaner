{
  "manifest_version": 3,
  "name": "Cookie Cleaner",
  "version": "1.0",
  "description": "Clean cookies while preserving whitelisted sites",
  "permissions": ["cookies", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
